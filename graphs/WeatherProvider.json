{
    "caseSensitive": false,
    "properties": {
        "name": "WeatherProvider",
        "environment": {
            "type": "noflo-nodejs"
        },
        "description": "Weather provider example for BIG IoT",
        "icon": "cloud",
        "main": true
    },
    "inports": {},
    "outports": {},
    "groups": [],
    "processes": {
        "LogErrors": {
            "component": "core/Output",
            "metadata": {
                "label": "LogErrors",
                "x": 684,
                "y": 324,
                "width": 72,
                "height": 72
            }
        },
        "SetupProvider": {
            "component": "flowhub-bigiot-bridge/Provider",
            "metadata": {
                "label": "SetupProvider",
                "x": 504,
                "y": 144,
                "width": 72,
                "height": 72
            }
        },
        "Configure": {
            "component": "flowhub-bigiot-bridge/CreateConfig",
            "metadata": {
                "label": "Configure",
                "x": 288,
                "y": 144,
                "width": 72,
                "height": 72
            }
        },
        "flowhub-bigiot-bridge/CreateOffering_a0s4a": {
            "component": "flowhub-bigiot-bridge/CreateOffering",
            "metadata": {
                "label": "CreateOffering",
                "x": 288,
                "y": 0,
                "width": 72,
                "height": 72
            }
        },
        "Temperature": {
            "component": "flowhub-bigiot-bridge/CreateDatatype",
            "metadata": {
                "label": "Temperature",
                "x": 72,
                "y": 0,
                "width": 72,
                "height": 72
            }
        },
        "flowhub-bigiot-bridge/GetMetar_tc4u5": {
            "component": "flowhub-bigiot-bridge/GetMetar",
            "metadata": {
                "label": "GetMetar",
                "x": 504,
                "y": 0,
                "width": 72,
                "height": 72
            }
        }
    },
    "connections": [
        {
            "src": {
                "process": "SetupProvider",
                "port": "error"
            },
            "tgt": {
                "process": "LogErrors",
                "port": "in"
            },
            "metadata": {
                "route": 1
            }
        },
        {
            "src": {
                "process": "Configure",
                "port": "out"
            },
            "tgt": {
                "process": "SetupProvider",
                "port": "config"
            },
            "metadata": {
                "route": 2
            }
        },
        {
            "src": {
                "process": "Configure",
                "port": "error"
            },
            "tgt": {
                "process": "LogErrors",
                "port": "in"
            },
            "metadata": {
                "route": 1
            }
        },
        {
            "src": {
                "process": "Temperature",
                "port": "out"
            },
            "tgt": {
                "process": "flowhub-bigiot-bridge/CreateOffering_a0s4a",
                "port": "outputdata"
            },
            "metadata": {
                "route": 3
            }
        },
        {
            "src": {
                "process": "flowhub-bigiot-bridge/CreateOffering_a0s4a",
                "port": "out"
            },
            "tgt": {
                "process": "SetupProvider",
                "port": "offering"
            },
            "metadata": {
                "route": 3
            }
        },
        {
            "src": {
                "process": "flowhub-bigiot-bridge/GetMetar_tc4u5",
                "port": "weather"
            },
            "tgt": {
                "process": "SetupProvider",
                "port": "response"
            },
            "metadata": {
                "route": 7
            }
        },
        {
            "src": {
                "process": "SetupProvider",
                "port": "request"
            },
            "tgt": {
                "process": "flowhub-bigiot-bridge/GetMetar_tc4u5",
                "port": "in"
            },
            "metadata": {
                "route": 6
            }
        },
        {
            "src": {
                "process": "flowhub-bigiot-bridge/GetMetar_tc4u5",
                "port": "error"
            },
            "tgt": {
                "process": "LogErrors",
                "port": "in"
            },
            "metadata": {
                "route": 1
            }
        },
        {
            "data": "localhost",
            "tgt": {
                "process": "Configure",
                "port": "hostname"
            }
        },
        {
            "data": "airportweather",
            "tgt": {
                "process": "flowhub-bigiot-bridge/CreateOffering_a0s4a",
                "port": "name"
            }
        },
        {
            "data": "/txl",
            "tgt": {
                "process": "flowhub-bigiot-bridge/CreateOffering_a0s4a",
                "port": "uri"
            }
        },
        {
            "data": "Berlin",
            "tgt": {
                "process": "flowhub-bigiot-bridge/CreateOffering_a0s4a",
                "port": "extent"
            }
        },
        {
            "data": "bigiot:weatherInformation",
            "tgt": {
                "process": "flowhub-bigiot-bridge/CreateOffering_a0s4a",
                "port": "rdfuri"
            }
        },
        {
            "data": "temperature",
            "tgt": {
                "process": "Temperature",
                "port": "name"
            }
        },
        {
            "data": "http://schema.org/airTemperatureValue",
            "tgt": {
                "process": "Temperature",
                "port": "rdfuri"
            }
        },
        {
            "data": "EDDT",
            "tgt": {
                "process": "flowhub-bigiot-bridge/GetMetar_tc4u5",
                "port": "station"
            }
        }
    ]
}
